{
  "name": "extravaganza",
  "version": "1.0.0",
  "description": "A random movie/series recommendation app according to rating and genre entered by the user.",
  "main": "index.js",
  "scripts": {
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "dev-server": "cd Backend && npm run dev",
    "dev-client": "cd client && npm run start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireQueen-3010/Extravaganza.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FireQueen-3010/Extravaganza/issues"
  },
  "homepage": "https://github.com/FireQueen-3010/Extravaganza#readme",
  "lint-staged": {
    "*.{js,json,css,scss,html,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "react-modal": "^3.14.3"
  }
